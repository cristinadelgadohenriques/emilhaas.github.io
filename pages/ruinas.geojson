---
layout: none
permalink: /ruinas.geojson
---
{
  "type": "FeatureCollection",
  "features": [
      {% for ruina in site.data.ruinas %}
      {
        "type": "Feature",
        "geometry": {
          "type": "Point",
          "coordinates": [{{ ruina.long }}, {{ruina.lat}}]
        },
        "properties": {
          "name": "{{ ruina.name }}",
        }

      } {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}



  ]
}
